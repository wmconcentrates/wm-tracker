# LeafLink API Configuration (Legacy - used if multi-tenant not configured)
LEAFLINK_API_KEY=your_api_key_here

# Server Configuration
PORT=3000

# Supabase Configuration (required for multi-tenant)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key_here

# API Key Encryption (IMPORTANT: Generate a secure 32+ character secret for production)
# You can generate one with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
API_KEY_ENCRYPTION_SECRET=change-this-to-a-secure-random-string

# Google Drive Service Account (for COA auto-import)
# Create a service account at console.cloud.google.com and paste the JSON key here
# The JSON must be on a single line (escape the newlines)
GOOGLE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"...","private_key":"..."}

# Google Sheets Label Export (for Canva label generation)
# Create a spreadsheet and share it with your service account email
# Copy the spreadsheet ID from the URL (between /d/ and /edit)
GOOGLE_LABEL_SPREADSHEET_ID=your_spreadsheet_id_here
